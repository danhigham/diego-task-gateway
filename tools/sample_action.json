{
  "rootfs": "docker:///busybox",
  "action": {
    "serial": {
      "actions": [
        {
          "run": {
            "path": "sh",
            "args": [
              "-c",
              "free -m > /tmp/result-file.txt;"
            ],
            "resource_limits": {},
            "user": "root",
            "suppress_log_output": false
          }
        }
      ]
    }
  },
  "disk_mb": 0,
  "memory_mb": 0,
  "cpu_weight": 0,
  "privileged": false,
  "log_source": "",
  "log_guid": "",
  "metrics_guid": "",
  "result_file": "/tmp/result-file.txt"
}
